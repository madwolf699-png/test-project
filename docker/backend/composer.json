{
  "name": "sun/backend",
  "description": "Backend project for your application",
  "type": "project",
  "require": {
    "php": ">=8.4"
  },
  "require-dev": {
    "phpdocumentor/phpdocumentor": "^3.9",
    "phpunit/phpunit": "^10.5",
    "friendsofphp/php-cs-fixer": "^3.50",
    "squizlabs/php_codesniffer": "^3.10",
    "phpstan/phpstan": "^1.11"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
