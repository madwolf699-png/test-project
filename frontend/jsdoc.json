{
  "source": {
    "include": ["src"],
    "exclude": ["src/main.tsx", "src/vite-config.ts", "src/vitest.setup.ts"],
    "includePattern": ".+\\.(js|jsx|ts|tsx)$",
    "excludePattern": "(node_modules|dist)"
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "template": "node_modules/tui-jsdoc-template"
  },
  "plugins": [],
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
